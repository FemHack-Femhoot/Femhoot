ot/src/App.js",[],"/Users/agustinaoh/code/agustinaoh/femhoot/src/components/DisplayResponse.js",["59"],"import React, { useState, useEffect } from \"react\";\n\nexport default function DisplayResponse({\n  responses,\n  rightAnswer,\n  setIsRightAnswer,\n}) {\n  const [randomResponses, setRandomResponses] = useState([]);\n  const [chosenAnswer, setChosenAnswer] = useState(\"\");\n\n  useEffect(() => {\n    if (chosenAnswer === rightAnswer) {\n      setIsRightAnswer(true);\n    } else {\n      setIsRightAnswer(false);\n    }\n  }, [chosenAnswer]);\n\n  function shuffleArray(array) {\n    for (var i = array.length - 1; i > 0; i--) {\n      var j = Math.floor(Math.random() * (i + 1));\n      var temp = array[i];\n      array[i] = array[j];\n      array[j] = temp;\n    }\n    return array;\n  }\n\n  useEffect(() => {\n    setRandomResponses(shuffleArray(responses));\n  }, [responses]);\n\n  return (\n    <div className=\"w-4/5 flex flex-col\">\n      {randomResponses.map((item) => (\n        <button\n          className=\"text-femhoot-blue font-semibold text-lg py-1 my-2 w-full border-2 border-femhoot-blue rounded-full hover:bg-femhoot-blue hover:text-white\"\n          key={item}\n          onClick={() => {\n            setChosenAnswer(item);\n          }}\n        >\n          {item}\n        </button>\n      ))}\n    </div>\n  );\n}\n","/Users/agustinaoh/code/agustinaoh/femhoot/src/components/DisplayFacts.js",[],"/Users/agustinaoh/code/agustinaoh/femhoot/src/components/Topbar.js",[],"/Users/agustinaoh/code/agustinaoh/femhoot/src/components/Footer.js",[],"/Users/agustinaoh/code/agustinaoh/femhoot/src/components/DisplayQuiz.js",["60","61","62"],"import React, { useState, useEffect } from \"react\";\nimport DisplayResponse from \"./DisplayResponse\";\nimport results from \"./../questions.json\";\n\nconst data = results.results;\nconst getRandomIndex = () => Math.floor(Math.random() * results.results.length);\n\nexport default function DisplayQuiz({ isEndGame, setIsEndGa